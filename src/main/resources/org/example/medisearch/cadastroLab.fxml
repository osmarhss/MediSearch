<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.medisearch.CadastrarLabController">

            <children>

                <Label layoutX="100.0" layoutY="110.0" text="Nome:" />
                <TextField fx:id="nomeField" layoutX="180.0" layoutY="110.0" prefWidth="320.0" />

                <Label layoutX="100.0" layoutY="150.0" text="CNPJ:" />
                <TextField fx:id="cnpjField" layoutX="180.0" layoutY="150.0" prefWidth="320.0" />

                <Label layoutX="100.0" layoutY="190.0" text="Endereço:" />
                <TextField fx:id="enderecoField" layoutX="180.0" layoutY="190.0" prefWidth="320.0" />

                <Label layoutX="100.0" layoutY="230.0" text="Telefone:" />
                <TextField fx:id="telefoneField" layoutX="180.0" layoutY="230.0" prefWidth="320.0" />

                <Button layoutX="285.0" layoutY="304.0" onAction="#cadastrarLaboratorio" text="Cadastrar"/>
                <Label layoutX="238.0" layoutY="48.0" text="Insira os dados do laboratório:" />
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="33.0" layoutX="-5.0" layoutY="-2.0" stroke="WHITE" strokeType="INSIDE" width="58.0" />
                <Label fx:id="voltar" layoutX="10.0" layoutY="5.0" onMouseClicked="#voltarTelaAnterior" prefHeight="17.0" prefWidth="36.0" text="Voltar" textFill="WHITE" />

            </children>

</AnchorPane>
