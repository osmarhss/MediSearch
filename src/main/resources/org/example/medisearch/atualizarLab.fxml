<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.medisearch.AtualizarLabController">
    <Label fx:id="attSucesso" layoutX="220.0" layoutY="352.0" text="Laboratório atualizado com sucesso!" visible="false" />
   <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#3da5ffd5" height="26.0" layoutX="3.0" layoutY="1.0" onMouseClicked="#voltarTelaAnterior" stroke="#ffffff00" strokeType="INSIDE" width="50.0">
      <effect>
         <DropShadow>
            <color>
               <Color opacity="0.5022796392440796" />
            </color>
         </DropShadow>
      </effect>
   </Rectangle>
    <TextField fx:id="idField" layoutX="180.0" layoutY="74.0" prefWidth="320.0" />

    <Label layoutX="100.0" layoutY="110.0" text="Nome:" />
    <TextField fx:id="nomeField" layoutX="180.0" layoutY="110.0" prefWidth="320.0" />

    <Label layoutX="100.0" layoutY="150.0" text="CNPJ:" />
    <TextField fx:id="cnpjField" layoutX="180.0" layoutY="150.0" prefWidth="320.0" />

    <Label layoutX="100.0" layoutY="190.0" text="Endereço:" />
    <TextField fx:id="enderecoField" layoutX="180.0" layoutY="190.0" prefWidth="320.0" />

    <Label layoutX="100.0" layoutY="230.0" text="Telefone:" />
    <TextField fx:id="telefoneField" layoutX="180.0" layoutY="230.0" prefWidth="320.0" />

    <Button layoutX="285.0" layoutY="304.0" onAction="#atualizarLaboratorio" text="Atualizar">
      <effect>
         <DropShadow>
            <color>
               <Color opacity="0.5509118437767029" />
            </color>
         </DropShadow>
      </effect></Button>
   <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#299bffc7" height="39.0" layoutX="203.0" layoutY="21.0" stroke="TRANSPARENT" strokeType="INSIDE" width="280.0">
      <effect>
         <DropShadow>
            <color>
               <Color opacity="0.578267514705658" />
            </color>
         </DropShadow>
      </effect>
   </Rectangle>
    <Label layoutX="211.0" layoutY="32.0" text="Insira os dados do laboratório para ser atualizado:" textFill="WHITE" />
    <Label fx:id="voltar" layoutX="10.0" layoutY="5.0" onMouseClicked="#voltarTelaAnterior" prefHeight="17.0" prefWidth="36.0" text="Voltar" textFill="WHITE" />
